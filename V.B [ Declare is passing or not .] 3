Private sub cmd result _ click ()
A= val (text1 . text)
B= val (text2. Text)
C=val (text3. Text)
D= val (text4. Text)
E= val (text5. Text)
F= val (text6. Text)
G = a+b+c+d+e+f
Text 7. Text g
h = g*100/400
text 8. Text = h 
if h>=60 then 
text 9. Text = “ 2nd class “
else 
if h > = 40 then 
text 9. Text = “3rd class”
