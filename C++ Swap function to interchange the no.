#include<iostream.h>
#include<conio.h>
void main()
{
clrscr();
int a,b;
void swap(int*x,int*y);
cout<<"enter the value of two numbers:";
cin>>a>>b;
cout<<"before A="<<a<<"b="<<b<<endl;
swap(&a,&b);
cout<<"after A="<<a<<"b="<<b<<endl;
